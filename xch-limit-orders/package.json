{
  "name": "xch-limit-order-extension",
  "version": "1.0.0",
  "description": "Automated limit orders for XCH purchases on vault.chia.net",
  "private": true,
  "scripts": {
    "dev:firefox": "web-ext run --source-dir . --firefox-profile dev-edition-default",
    "dev:chrome": "echo 'Load unpacked extension from chrome://extensions'",
    "build": "web-ext build --source-dir . --artifacts-dir dist",
    "lint": "web-ext lint --source-dir .",
    "test": "echo 'No tests configured'"
  },
  "devDependencies": {
    "web-ext": "^8.0.0"
  },
  "keywords": [
    "browser-extension",
    "chia",
    "xch",
    "limit-orders",
    "automation"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  }
}
